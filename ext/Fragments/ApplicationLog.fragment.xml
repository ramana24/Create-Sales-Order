<core:FragmentDefinition xmlns:core="sap.ui.core" xmlns="sap.m">
	<Dialog id="Dialog" title="{i18n>applicationLogDialogTitle}" contentHeight="45%" contentWidth="60%" draggable="true" resizable="true">
